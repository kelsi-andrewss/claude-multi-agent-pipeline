{
  "permissions": {
    "allow": [
      "Bash(/opt/homebrew/opt/claude-code-tracker/libexec/src/stop-hook.sh*)",
      "Bash(/Users/kelsiandrews/.claude/hooks/context-check.sh*)",
      "Bash(/Users/kelsiandrews/.claude/hooks/cost-alert.sh*)",
      "Bash(/Users/kelsiandrews/.claude/hooks/guard-protected-files.sh*)"
    ]
  },
  "hooks": {
    "PreToolUse": [
      {
        "matcher": "Edit|Write",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/kelsiandrews/.claude/hooks/guard-direct-edit.sh",
            "timeout": 5
          },
          {
            "type": "command",
            "command": "/Users/kelsiandrews/.claude/hooks/guard-protected-files.sh",
            "timeout": 5
          }
        ]
      },
      {
        "matcher": "Task",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/kelsiandrews/.claude/hooks/require-orch-read.sh",
            "timeout": 5
          }
        ]
      },
      {
        "matcher": "Bash",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/kelsiandrews/.claude/hooks/warn-sync-heavy-bash.sh",
            "timeout": 5,
            "async": true
          }
        ]
      }
    ],
    "PostToolUse": [
      {
        "matcher": "Read",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/kelsiandrews/.claude/hooks/mark-orch-read.sh",
            "timeout": 5,
            "async": true
          }
        ]
      },
      {
        "matcher": "TaskUpdate",
        "hooks": [
          {
            "type": "command",
            "command": "/Users/kelsiandrews/.claude/hooks/context-check.sh",
            "timeout": 5,
            "async": true
          }
        ]
      }
    ],
    "Stop": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "/opt/homebrew/opt/claude-code-tracker/libexec/src/stop-hook.sh",
            "timeout": 30,
            "async": true
          }
        ]
      },
      {
        "hooks": [
          {
            "type": "command",
            "command": "/Users/kelsiandrews/.claude/hooks/cost-alert.sh",
            "timeout": 10,
            "async": true
          }
        ]
      }
    ],
    "SessionStart": [
      {
        "hooks": [
          {
            "type": "command",
            "command": "/opt/homebrew/opt/claude-code-tracker/libexec/src/stop-hook.sh --backfill-only",
            "timeout": 60,
            "async": true
          }
        ]
      },
      {
        "hooks": [
          {
            "type": "command",
            "command": "/Users/kelsiandrews/.claude/hooks/load-session-context.sh",
            "timeout": 10
          }
        ]
      }
    ]
  },
  "enabledPlugins": {
    "ralph-loop@claude-plugins-official": true
  },
  "alwaysThinkingEnabled": false,
  "skipDangerousModePermissionPrompt": true,
  "model": "sonnet"
}
